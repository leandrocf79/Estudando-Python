# Conta o número de vezes que cada número apareceu
counts = Counter(numbers)

# Calcula a probabilidade de cada número ser sorteado nas próximas 
for number, count in counts.items():
  probability = count / len(numbers)
  print(f"A probabilidade do número {number} ser sorteado é de {probability:.2f}%")
